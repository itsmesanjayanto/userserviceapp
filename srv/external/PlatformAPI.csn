      "@Common.Label": "SCIM users - shadow users",
      "@Core.Description": "Creates a user.",
      "@Core.LongDescription": "Creates a user with the properties specified in the body. **To create a group membership (assign role collections), use the endpoint [/Groups/{Id}/members](https://docs.cloudfoundry.org/api/uaa/version/74.4.0/index.html#add-member)**.",
      "@openapi.path": "/Users",
      "returns": {
        "type": "User.Management._System.for.Cross.domain.Identity.Management._SCIM___types.ScimUserPOST_PUT"
      }
    },
